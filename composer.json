{
    "name": "optimistdigital/nova-multiselect-field",
    "description": "A multiple select field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "multiselect",
        "select"
    ],
    "authors": [
        {
            "name": "Tarvo Reinpalu",
            "email": "tarvoreinpalu@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Optimist Digital",
            "email": "info@optimistdigital.com",
            "role": "Maintainer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "workup/nova": "^3.0",
        "workup/nova-translations-loader": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Workup\\Nova\\MultiselectField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\Nova\\MultiselectField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/workupsrl/nova"
        }
    ]
}
